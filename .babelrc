{
  "presets": [
    "es2015",
    "stage-2",
    "env",
    "flow"
  ],
  "plugins": [
    [
      "transform-flow-strip-types"
    ],
    [
      "transform-class-properties"
    ],
    [
      "module-resolver",
      {
        "root": [
          "."
        ],
        "alias": {
          "services": "./src/services",
          "methods": "./src/methods",
          "controllers": "./src/controllers",
          "constants": "./src/constants",
          "utils": "./src/utils",
          "sockets": "./src/sockets",
          "__tests__": "./__tests__",
          "__mocks__": "./__mocks__",
          "test": "./src/test"
        },
        "cwd": "babelrc"
      }
    ]
  ]
}